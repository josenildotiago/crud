{
    "name": "josenildotiago/crud",
    "description": "Modern CRUD generator with dynamic themes for Laravel 12 + React.js",
    "version": "3.0.2",
    "license": "MIT",
    "authors": [
        {
            "name": "josenildotiagosilva",
            "email": "josenildo.tiago.designer@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Crud\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Crud\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.2.0",
        "illuminate/console": "^12.0",
        "illuminate/filesystem": "^12.0",
        "illuminate/support": "^12.0",
        "illuminate/validation": "^12.0",
        "illuminate/database": "^12.0",
        "symfony/console": "^7.0",
        "laravel/prompts": "^0.3.0"
    },
    "require-dev": {
        "orchestra/testbench": "^10.0",
        "phpunit/phpunit": "^11.0",
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Crud\\CrudServiceProvider"
            ],
            "aliases": {
                "Crud": "Crud\\Facades\\Crud"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
